# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here
# Optional: for migrations (Supabase Dashboard → Project Settings → Database → Connection string)
# DATABASE_URL=postgresql://postgres:[PASSWORD]@db.[PROJECT_REF].supabase.co:5432/postgres

# CORS Configuration
# Development: ALLOWED_ORIGINS=*
# Production: ALLOWED_ORIGINS=https://yourdomain.com
ALLOWED_ORIGINS=*

# Rate Limiting - Trusted Proxy IPs
# Leave empty for direct connections (prevents spoofing)
# TRUSTED_PROXIES=127.0.0.1,10.0.0.1
TRUSTED_PROXIES=

# Optional: Model Configuration
# MODEL_NAME=gemini-3-flash-preview
# ENABLE_HYBRID_MODE=true

# Batch processing concurrency (optional, default: 1)
BATCH_WORKERS=1
BATCH_API_LIMIT=3
