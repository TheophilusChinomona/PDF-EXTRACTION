# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here
# Service role key for scripts (bypasses RLS). Get from Dashboard → Project Settings → API
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
# Optional: for migrations (Supabase Dashboard → Project Settings → Database → Connection string)
# DATABASE_URL=postgresql://postgres:[PASSWORD]@db.[PROJECT_REF].supabase.co:5432/postgres

# Firebase/GCS Configuration (for storage extraction)
# Path to service account JSON file OR the JSON string itself
FIREBASE_CREDENTIALS_PATH=/path/to/serviceAccountKey.json
# Alternatively: FIREBASE_SERVICE_ACCOUNT_JSON={"type":"service_account",...}

# CORS Configuration
# Development: ALLOWED_ORIGINS=*
# Production: ALLOWED_ORIGINS=https://yourdomain.com
ALLOWED_ORIGINS=*

# Rate Limiting - Trusted Proxy IPs
# Leave empty for direct connections (prevents spoofing)
# TRUSTED_PROXIES=127.0.0.1,10.0.0.1
TRUSTED_PROXIES=

# Optional: Model Configuration
# MODEL_NAME=gemini-3-flash-preview
# ENABLE_HYBRID_MODE=true

# Batch processing concurrency (optional, default: 1)
BATCH_WORKERS=1
BATCH_API_LIMIT=3

# Gemini Batch API (optional, for 100+ file batches)
# BATCH_API_THRESHOLD=100
# BATCH_API_POLL_INTERVAL=60
# BATCH_API_MODEL=models/gemini-2.5-flash
